  const confidentialText = document.querySelector('.js-confidential-content')
  const confidentialCheckbox = document.querySelector('.js-confidential-checkbox');
  const submitButton = document.querySelector('.js-submit-button-with-confidential');

  function setDisabledSubmitButton (flag) {
    if (flag) {
      submitButton && submitButton.removeAttribute('disabled');
      confidentialText.classList.remove('gb-confidential-policy-form-links__content--error');
    } else {
      submitButton && submitButton.setAttribute('disabled', 'disabled');
      confidentialText.classList.add('gb-confidential-policy-form-links__content--error')
    }
  }

  const socialButtons = document.querySelectorAll('.js-social-button-with-confidential');
  const socialLinks = {};

  socialButtons.length && socialButtons.forEach(link => {
    socialLinks[link.getAttribute('data-social-name')] = link.getAttribute('href');
  });

  function setDisabledSocialButtons (flag) {
    if (flag) {
      socialButtons.length && socialButtons.forEach(link => {
        link.removeAttribute('disabled');
        link.setAttribute('href', socialLinks[link.getAttribute('data-social-name')])
      });
    } else {
      socialButtons.length && socialButtons.forEach(link => {
        link.setAttribute('disabled', 'true')
        link.removeAttribute('href')
      });
    }
  }

  confidentialCheckbox.addEventListener('change', (e) => {
    const { checked } = e.target;

    setDisabledSubmitButton(checked);
    setDisabledSocialButtons(checked);
  });

