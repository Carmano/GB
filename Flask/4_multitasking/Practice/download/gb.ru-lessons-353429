  function getCookie(name) {
    const matches = document.cookie.match(
      new RegExp(
        `(?:^|; )${name.replace(/([.$?*|{}()[\]\\/+^])/g, '\\$1')}=([^;]*)`,
      ),
    );

    return matches ? decodeURIComponent(matches[1]) : undefined;
  }

  const orderRef = getCookie('cartRef');
  const orderAccessToken = getCookie('cartAccessToken');
  const userId = getCookie('user_id');

  const classes = ['.mn-dropdown-item__cart-item', '.mn-menu-nested-list__cart-item']

  const displayCartItem = (classes, items_count) => {
    classes.forEach((value) => {
      const cartHtmlElem = document.getElementById('header-monolit').querySelector(value)

      if (cartHtmlElem) {
        cartHtmlElem.innerHTML = items_count
        cartHtmlElem.style.display = 'block'
      }
    })
  }

  const getAuthCart = (headers) => {
    fetch('/api/v2/cart/items_count', {headers})
      .then(response => response.json())
      .then(data => {
        if (data.items_count){
          displayCartItem(classes, data.items_count)
        }
      })
      .catch(e => global.globalJS.Sentry.captureException(e));
  };


  const getItemsCount = () => {
    if (orderRef && orderAccessToken) {
      return getAuthCart({
        'X-Order-Ref': orderRef,
        'X-Order-Access-Token': orderAccessToken,
      });
    }

    if (userId) getAuthCart();
  };

  getItemsCount()
}</script></body><script type="module" src="https://frontend-scripts.hb.bizmrg.com/components/phoneNumberInput.js"></script></html>