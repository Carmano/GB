  const phoneInput = document.querySelector('.gb-phone-input-hidden')
  const emailSubmitButton = document.querySelector('.js-submit-button-with-confidential.email-btn');
  const smsSubmitButton = document.querySelector('.js-submit-button-with-confidential.sms-btn');

  function setDisabledSubmitButton(btn, val) {
    if (val) {
      btn && btn.removeAttribute('disabled');
    } else {
      btn && btn.setAttribute('disabled', 'disabled');
    }
  }

  setDisabledSubmitButton(emailSubmitButton, phoneInput.value)
  setDisabledSubmitButton(smsSubmitButton, phoneInput.value)

  phoneInput.addEventListener('input', (event) => {
    setDisabledSubmitButton(emailSubmitButton, event.target.value)
    setDisabledSubmitButton(smsSubmitButton, event.target.value)
  })

  function smsRegisterPrepareData() {
    const phone = document.getElementsByClassName('gb-phone-input-hidden')[0].value
    document.getElementById('user_sms_registration').value = true;
    document.getElementById('user_reg_phone').value = phone;
  }

  if(smsSubmitButton) {
    smsSubmitButton.onclick = smsRegisterPrepareData
  }
